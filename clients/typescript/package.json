{
  "name": "@ormdb/client",
  "version": "0.1.0",
  "description": "TypeScript client for ORMDB with ORM adapters",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./prisma": {
      "import": "./dist/prisma/index.mjs",
      "require": "./dist/prisma/index.js",
      "types": "./dist/prisma/index.d.ts"
    },
    "./drizzle": {
      "import": "./dist/drizzle/index.mjs",
      "require": "./dist/drizzle/index.js",
      "types": "./dist/drizzle/index.d.ts"
    },
    "./typeorm": {
      "import": "./dist/typeorm/index.mjs",
      "require": "./dist/typeorm/index.js",
      "types": "./dist/typeorm/index.d.ts"
    },
    "./kysely": {
      "import": "./dist/kysely/index.mjs",
      "require": "./dist/kysely/index.js",
      "types": "./dist/kysely/index.d.ts"
    },
    "./sequelize": {
      "import": "./dist/sequelize/index.mjs",
      "require": "./dist/sequelize/index.js",
      "types": "./dist/sequelize/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ormdb",
    "database",
    "orm",
    "prisma",
    "drizzle",
    "typeorm",
    "kysely",
    "sequelize",
    "typescript"
  ],
  "author": "ORMDB Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ormdb/ormdb"
  },
  "dependencies": {},
  "peerDependencies": {
    "@prisma/client": ">=5.0.0",
    "drizzle-orm": ">=0.29.0",
    "kysely": ">=0.27.0",
    "sequelize": ">=6.0.0",
    "typeorm": ">=0.3.0"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    },
    "kysely": {
      "optional": true
    },
    "sequelize": {
      "optional": true
    },
    "typeorm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@prisma/client": "^5.0.0",
    "@types/node": "^20.0.0",
    "drizzle-orm": "^0.29.0",
    "kysely": "^0.27.0",
    "sequelize": "^6.35.0",
    "tsup": "^8.0.0",
    "typeorm": "^0.3.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
